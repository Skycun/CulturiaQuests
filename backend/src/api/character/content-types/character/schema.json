{
  "kind": "collectionType",
  "collectionName": "characters",
  "info": {
    "singularName": "character",
    "pluralName": "characters",
    "displayName": "Character"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "job": {
      "type": "enumeration",
      "enum": ["hero", "mage", "archer", "soldier"],
      "required": true
    },
    "guild": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::guild.guild",
      "inversedBy": "characters"
    },
    "items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::item.item",
      "mappedBy": "character"
    }
  }
}
