{
  "kind": "collectionType",
  "collectionName": "npcs",
  "info": {
    "singularName": "npc",
    "pluralName": "npcs",
    "displayName": "Npc"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "pronouns": {
      "type": "enumeration",
      "required": true,
      "default": "they",
      "enum": [
        "he",
        "she",
        "they"
      ]
    },
    "quests_entry_available": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "expedition_entry_available": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "friendships": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::friendship.friendship",
      "mappedBy": "npc"
    },
    "quests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quest.quest",
      "mappedBy": "npc"
    },
    "dialogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::dialog.dialog",
      "mappedBy": "npc"
    },
    "runs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::run.run",
      "mappedBy": "npc"
    }
  }
}
