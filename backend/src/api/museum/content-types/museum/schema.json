{
  "kind": "collectionType",
  "collectionName": "museums",
  "info": {
    "singularName": "museum",
    "pluralName": "museums",
    "displayName": "Museum"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "lat": {
      "type": "float",
      "required": false
    },
    "lng": {
      "type": "float",
      "required": false
    },
    "geohash": {
      "type": "string"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "museums"
    },
    "runs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::run.run",
      "mappedBy": "museum"
    },
    "location": {
      "type": "customField",
      "customField": "plugin::geodata.geojson",
      "options": {
        "info": true
      }
    },
    "radius": {
      "type": "integer"
    }
  }
}
