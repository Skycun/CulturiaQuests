{
  "name": "populate-db",
  "version": "1.0.0",
  "description": "Generic script to populate Strapi database",
  "type": "module",
  "scripts": {
    "populate": "tsx src/index.ts",
    "populate:dialog": "tsx src/index.ts --type dialog --input ./data/npc_entries.json",
    "populate:dialog:dry": "tsx src/index.ts --type dialog --input ./data/npc_entries.json --dry-run"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
